@{
    ViewBag.Title = "List PO";
    Layout = "~/Views/Shared/_InventoryLayout.cshtml";
}
<link href="~/Content/jquery-ui.css" rel="stylesheet" />
<div class="row top-head">
    <div class="col-md-12"><p><b><a style="color:#fff;" href="~/Dashboard/PurchaseDashboard">Purchase </a> >> List Approved Store Requisitions Without PO </b></p></div>
</div>
<section>
    <form action="#" id="frmAddEditPO" class="form-horizontal as" enctype="multipart/form-data" method="post" accept-charset="utf-8">
        <div class="row inner">
            <div class="message">

            </div>
            
            <div class="col-md-12 ftop">
                <div class="form-group  col-md-6">
                    <label for="txtRequisitionNo" class="col-sm-4 control-label">Requisition No.</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtRequisitionNo" name="txtRequisitionNo" tabindex="1" placeholder="Search with Requisition No." type="text" maxlength="50" />
                    </div>

                </div>
                <div class="form-group col-md-6">
                   
                </div>
                

            </div>

            <div class="col-md-12">
                <div class="form-group col-md-6">
                    <label for="txtCustomerName" class="col-sm-4 control-label">Customer Name</label>
                    <div class="col-sm-8">
                        <input class="form-control" id="txtCustomerName" name="txtCustomerName" tabindex="11" placeholder="Enter Customer Name" type="text" maxlength="100" />
                        <input id="hdnCustomerId" type="hidden" value="0" />
                    </div>
                </div>
                <div class="form-group col-md-6">
                    <label for="txtCustomerCode" class="col-sm-4 control-label">Customer Site</label>
                    <div class="col-sm-8">
                        <select id="ddlCustomerBranch" name="ddlCustomerBranch" tabindex="9" class="form-control"></select>
                        <input id="hdnCustomerBranchId" type="hidden" value="0" />
                    </div>
                </div>
            </div>

            <div class="col-md-12">
                <div class="form-group col-md-6">
                    <label for="txtFromDate" class="col-sm-4 control-label">From Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtFromDate" name="txtFromDate" tabindex="5" value="@ViewData["fromDate"]" type="text" maxlength="50" />
                        <input id="hdnFromDate" name="hdnFromDate" value="@ViewData["fromDate"]" type="hidden" />
                    </div>
                </div>
                <div class="form-group col-md-6">
                    <label for="txtToDate" class="col-sm-4 control-label">To Date</label>
                    <div class="col-sm-8 ">
                        <input class="form-control" id="txtToDate" name="txtToDate" tabindex="6" value="@ViewData["toDate"]" type="text" maxlength="50" />
                        <input id="hdnToDate" name="hdnToDate" value="@ViewData["toDate"]" type="hidden" />
                    </div>
                </div>
            </div>



            <div class="row col-sm-12 col-lg-12 col-md-12">
                <div class="col-md-3"></div>
                <div class="col-md-6">
                    <center>
                        <a class="btn btn-success" target="_blank" tabindex="9" href="#" id="lnkGenerateReport">Generate Report</a>
                    </center>
                </div>
                <div class="col-md-3"></div>
            </div>
            <div class="row col-sm-6 col-lg-12 col-md-12">
                <div id="divList">

                </div>
            </div>
        </div>
        <!-- Modal -->
        <div class="modal fade" id="alertModel" role="dialog">
            <div class="modal-dialog  modal-sm">
                <!-- Modal content-->
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal">&times;</button>
                        <h4 class="modal-title" id="modelHeader"></h4>
                    </div>
                    <div class="modal-body">
                        <p id="modelText"></p>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-primary  btn-block" data-dismiss="modal">Close</button>
                    </div>
                </div>

            </div>
        </div>
    </form>
</section>
<script src="~/Scripts/StoreRequisition/ListApprovedStoreRequisitionsWithoutPO.js"></script>
<script src="~/Scripts/jquery-ui.min.js"></script>
<script src="~/Scripts/paging.js"></script>
<script type="text/javascript">
    $("body").on('click', '.dropdown-toggle', function () {
        $(".dropdown-toggle").parent().toggleClass('open');
    });

</script>